    "mochawesome-report-generator": "^5.2.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "react-scripts start",
    "prebuild": "rm -rf build/ dist/",
    "build": "react-scripts build",
    "postbuild": "mv build/ dist/",
    "eject": "react-scripts eject",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!@elninotech/mfd-modules)/\"",
    "serve": "serve -C -s -l 8000 dist",
    "test:dev": "npm test -- --watchAll=false && cypress run",
    "test:unit": "npm test",
    "test:e2e": "cypress run",
    "test:ci": "npm test -- --watchAll=false && cypress run",
    "generate-e2e-report": "npm run copy-e2e-assets && npm run merge-e2e-results && npm run create-e2e-html-report",
    "copy-e2e-assets": "rm -rf cypress/report && mkdir cypress/report && cp -r cypress/screenshots cypress/videos cypress/report/",
    "merge-e2e-results": "mochawesome-merge cypress/results/*.json > cypress/results/results.json",
    "create-e2e-html-report": "marge cypress/results/results.json -f index.html -o cypress/report --cdn",
    "cy:version": "cypress version",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prepare": "husky install",
    "po:push": "node src/app/locales/poeditor/push.js",
    "po:pull": "node src/app/locales/poeditor/pull.js"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "last 2 versions"
    ],
    "development": [
      "chrome >= 45",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
